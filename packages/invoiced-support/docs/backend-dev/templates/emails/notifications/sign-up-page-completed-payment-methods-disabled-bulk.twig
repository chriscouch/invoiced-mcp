{% extends "emails/notifications/parent.twig" %}
{% block title %}New sign up page completions <br> Some payment methods were disabled{% endblock %}
{% block notification %}
    {% for customer in customers %}
        <p><a href="{{ dashboardUrl }}/alerts?account={{ tenant_id  }}">{{ customer.cnt }} customers completed a sign up page</a></p>
    {% endfor %}

    <p>Some payment methods were disabled because the payment gateway is not properly configured.</p>
{% endblock %}