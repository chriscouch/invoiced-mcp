{% extends "emails/notifications/parent.twig" %}
{% block title %}Sign up page completed <br/> Some payment methods were disabled{% endblock %}
{% block notification %}
    {% for customer in customers %}
        <p><a href="{{ dashboardUrl}}/customers/{{ customer.id  }}?account={{ tenant_id  }}">{{ customer.name }}</a> has completed your sign up page{% if customer.sign_up_page %}: {{ customer.sign_up_page.name }}{% endif %} <br/><br/> Some payment methods were disabled because the payment gateway is not properly configured.</p>
    {% endfor %}
{% endblock %}