<!DOCTYPE html>
<html {% if htmlClass is defined %}class="{{ htmlClass }}"{% endif %}>
<head>
    <title>{% if title is defined %}{{ title }}{% endif %}{% if noTitleAppend is not defined or not(noTitleAppend) %}{% if title is defined %} - {%  endif %}Invoiced{% endif %}</title>

    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
    <meta name="robots" content="{{ robots|default(robotsDefault) }}" />
    <meta name="keywords" content="invoices,billing,payments,subscription billing,subscription billing"/>
    <meta name="description" content="{{ metaDescription|default('Invoiced is the leading accounts receivable and accounts payable automation platform.') }}"/>
    {% if noBaseUrl is not defined or not(noBaseUrl) %}<base href="{{ app.request.schemeAndHttpHost ~ app.request.baseUrl }}" />{% endif %}
    {% if canonicalUrl is defined %}<link rel="canonical" href="{{ canonicalUrl }}" />{% endif %}

    <meta property="og:title" content="{{ title|default('Invoiced') }}" />
    <meta property="og:type" content="{{ ogType|default('website') }}" />
    <meta property="og:description" content="{{ metaDescription|default('Invoiced is the leading accounts receivable and accounts payable automation platform.') }}" />
    <meta property="og:image" content="{{ ogImage|default('https://www.invoiced.com/img/landing2/social-media-image.png') }}" />
    <meta property="og:logo" content="https://www.invoiced.com/img/icon-512.png" />
    <meta property="og:site_name" content="{{ ogSiteName|default('Invoiced') }}" />
    <meta property="fb:admins" content="100005755443095" />
    <meta name="p:domain_verify" content="58bdda47b4e31ad0779cbe34e34ce1be"/>

    <link rel="apple-touch-icon" sizes="57x57" href="{{ asset('/apple-icon-57x57.png') }}"/>
    <link rel="apple-touch-icon" sizes="60x60" href="{{ asset('/apple-icon-60x60.png') }}"/>
    <link rel="apple-touch-icon" sizes="72x72" href="{{ asset('/apple-icon-72x72.png') }}"/>
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('/apple-icon-76x76.png') }}"/>
    <link rel="apple-touch-icon" sizes="114x114" href="{{ asset('/apple-icon-114x114.png') }}"/>
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('/apple-icon-120x120.png') }}"/>
    <link rel="apple-touch-icon" sizes="144x144" href="{{ asset('/apple-icon-144x144.png') }}"/>
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('/apple-icon-152x152.png') }}"/>
    <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('/apple-icon-180x180.png') }}"/>
    <link rel="icon" type="image/png" sizes="192x192"  href="{{ asset('/android-icon-192x192.png') }}"/>
    <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('/favicon-32x32.png') }}"/>
    <link rel="icon" type="image/png" sizes="96x96" href="{{ asset('/favicon-96x96.png') }}"/>
    <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('/favicon-16x16.png') }}"/>
    <link rel="manifest" href="{{ asset('/manifest.json') }}"/>
    <meta name="msapplication-TileColor" content="#ffffff"/>
    <meta name="msapplication-TileImage" content="{{ asset('/ms-icon-144x144.png') }}"/>
    <meta name="theme-color" content="#ffffff"/>

    <link href="{{ asset("/dashkit/css/theme.bundle.css") }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('/css/network.css') }}" rel="stylesheet" type="text/css" />

    <script type="text/javascript" src="{{ asset('/js/jquery.min.js') }}"></script>

    {% block header %}{% endblock %}
</head>
<body class="{% block bodyClass %}{% endblock %}">
{% block main %}{% endblock %}
</body>
</html>
