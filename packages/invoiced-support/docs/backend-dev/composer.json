{
    "name": "invoiced/invoiced-core",
    "description": "Backend for Invoiced",
    "license": "proprietary",
    "homepage": "https://invoiced.com",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": false,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {"App\\": "src/"},
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Invoiced/php-resque"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Invoiced/ringphp-guzzle-handler"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Invoiced/bai2"
        },
        {
            "type": "vcs",
            "url": "https://github.com/RelistenNL/forceutf8"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pcntl": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "ext-xsl": "*",
        "authorizenet/authorizenet": "2.0.2",
        "authy/php": "2.*",
        "avalara/avataxclient": "^22.0",
        "aws/aws-php-sns-message-validator": "*",
        "aws/aws-sdk-php": "~3.18",
        "aws/aws-sdk-php-symfony": "^2.1",
        "braintree/braintree_php": "^3.26",
        "chartmogul/chartmogul-php": "^6.0",
        "chrisboulton/php-resque": "dev-php7_errors as dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master#5954c989026f1bbc6443c02078a655333e152242",
        "commerceguys/addressing": "v2.1.1",
        "composer/package-versions-deprecated": "1.11.99.5",
        "datadog/php-datadogstatsd": " 1.0.*",
        "defuse/php-encryption": "~2.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "dragonmantank/cron-expression": "^3.1",
        "elasticsearch/elasticsearch": "^7.10",
        "firebase/php-jwt": "^6.4",
        "florianv/exchanger": "^2.7.2",
        "florianv/swap": "^4.3",
        "giggsey/libphonenumber-for-php": "^8.13",
        "gocardless/gocardless-pro": "^5.0",
        "google/recaptcha": "~1.1",
        "greenter/ubl-validator": "^2.1",
        "guzzlehttp/guzzle": "^7",
        "icanboogie/inflector": "^2.0",
        "imagine/imagine": "1.0.1",
        "intacct/intacct-sdk-php": "^v3.0",
        "invoiced/invoiced": "dev-master#4857eedc559c3008776db57ff663a5d2d8f84379",
        "jaqb/jaqb": "^1.6",
        "jaybizzle/crawler-detect": "^1.2",
        "jenssegers/agent": "^2.6",
        "league/oauth2-server": "^8.3",
        "lob/lob-php": "^3.0",
        "lukewaite/ringphp-guzzle-handler": "dev-fix-type-error as 2.2",
        "martinssipenko/password-policy": "^1.0",
        "maxbanton/cwh": "^2.0",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "moneyphp/money": "^4.0",
        "monolog/monolog": "1.26.1 as 2.5.0",
        "mustache/mustache": "2.14.1",
        "nacha/file-generator": "^1.9",
        "neitanod/forceutf8": "dev-master#9910e47e9c2a21cc79df0d27e03d560e4113a33e",
        "nelmio/cors-bundle": "^2.2",
        "nesbot/carbon": "^2.48",
        "nyholm/psr7": "^1.5",
        "onelogin/php-saml": "^4.0",
        "paragonie/csp-builder": "~2.0",
        "php-mime-mail-parser/php-mime-mail-parser": "8.0.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-symfony": "^1.1",
        "rlanvin/php-rrule": "^2.4",
        "robmorgan/phinx": "^0.12.10",
        "ryanwinchester/netsuite-php": "2017.1.*",
        "sentry/sentry-symfony": "^4.0",
        "smalot/pdfparser": "^0.14.0",
        "stechstudio/bai2": "dev-main",
        "stripe/stripe-php": "^15.0",
        "symfony-bundles/json-request-bundle": "^4.1",
        "symfony/amazon-mailer": "6.4.*",
        "symfony/asset": "^6.4",
        "symfony/cache": "^6.4",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/event-dispatcher": "^6.4",
        "symfony/expression-language": "^6.4",
        "symfony/flex": "^1.1",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "^6.4",
        "symfony/lock": "^6.4",
        "symfony/mailer": "^6.4",
        "symfony/monolog-bundle": "^3.3",
        "symfony/postmark-mailer": "6.4.*",
        "symfony/process": "^6.4",
        "symfony/proxy-manager-bridge": "^6.4",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/rate-limiter": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-csrf": "^6.4",
        "symfony/semaphore": "^5.2",
        "symfony/sendgrid-mailer": "6.4.*",
        "symfony/stopwatch": "^6.4",
        "symfony/translation": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/validator": "6.4.*",
        "symfony/yaml": "^6.4",
        "thiagoalessio/tesseract_ocr": "^2.9",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^3.4",
        "twig/inky-extra": "^3.4",
        "twilio/sdk": "^8.0",
        "willdurand/email-reply-parser": "^2.8"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.4",
        "rector/rector": "^0.19",
        "symfony/browser-kit": "^6.4",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^6.4",
        "symfony/web-profiler-bundle": "^6.4"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "runtime": {
            "prod_envs": ["prod", "production", "sandbox", "staging"],
            "use_putenv": true
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
